{
  "name": "@nicolawealth/ioc",
  "version": "0.1.2",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "files": ["."],
  "author": "Nicola Wealth Inc",
  "contributors": [
    {
      "name": "Cliff Hammerschmidt"
    },
    {
      "name": "Skye Haik"
    },
    {
      "name": "Travis Rempel"
    },
    {
      "name": "Mel Delosada"
    }
  ],
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/* && rm -f tsconfig.tsbuildinfo",
    "build": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "lint": "eslint src",
    "test": "mocha",
    "test-r": "mocha -R spec",
    "cover": "nyc mocha",
    "cover:report": "nyc --reporter=html --reporter=lcov --reporter=text --reporter=json mocha -c --reporter=spec",
    "doc": "npx typedoc src/*.ts src/**/*.ts --out dist/doc"
  },
  "engines": {
    "node": "^22"
  },
  "dependencies": {
    "@nicolawealth/defer_until": "^0.1.1"
  },
  "devDependencies": {
    "@types/mocha": "^9",
    "@types/node": "^22",
    "@types/sinon": "^10",
    "@typescript-eslint/eslint-plugin": "^8",
    "eslint": "^9",
    "eslint-config-turbo": "^2",
    "mocha": "^10",
    "mocha-junit-reporter": "^2",
    "nyc": "^15",
    "sinon": "^19",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "packageManager": "npm@10.9.0"
}
